{
  "name": "smarter-experiences",
  "version": "1.0.0",
  "description": "Starter application of Angular2 with Express",
  "main": "dist/server/bin/www.js",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json & npm run _server:run",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build --prod --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"ngc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "ngc -p ./server",
    "production": "NODE_ENV=production node dist/server/bin/www.js"
  },
  "author": "Vlado Tešanović",
  "private": true,
